package 배열;

public class Ex02배열기초 {

	public static void main(String[] args) {

	int[] numbers = new int[5];
	
	// 배열의 길이
	// 변수명.length;
	int length = numbers.length;
	System.out.println("배열의 길이 : " + length);
	
	// 배열의 각 칸을 5의 배수로 초기화해 주세요.
	numbers[0] = 5;
	numbers[1] = 10;
	numbers[2] = 15;
	numbers[3] = 20;
	numbers[4] = 25;
	
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
