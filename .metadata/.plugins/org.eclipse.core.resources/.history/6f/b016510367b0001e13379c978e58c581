package 보충0111;

public class Ex01 {

	public static void main(String[] args) {
		// for : 반복 횟수가 정해져 있을 때 (~언제부터 ~언제까지)
		// i=0 .... 0
		// i=1 .... 1
		// i=2 .... 2
		// i=3 
		for(int i=0; i<3; i++) {
			System.out.println(i);
		}
		
		// 1부터 9까지 출력 시켜주세요
		for(int i=0; i<9; i++) {
			System.out.println(i+1);
		}
		
		
		
		
		
	}

}
