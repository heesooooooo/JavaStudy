package 사;

import java.util.Scanner;

public class Ex01별찍기행개수 {

	public static void main(String[] args) {

		Scanner sc= new Scanner(System.in);
		System.out.print("행 개수 : ");
		int input = sc.nextInt();
		
		for(int i=0; i<input; i++) {
			System.out.println("*");
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
