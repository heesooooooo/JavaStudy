package 배열;

import java.util.Random;

public class Ex04가장큰수찾기 {

	public static void main(String[] args) {

		int[] array = new int[5];
		Random ran = new Random();
		System.out.print("배열 안에 들어있는 값 : ");
		for(int i =0; i<5; i++) {
			array[i] = ran.nextInt(10)+1;
			System.out.print(array[i]+" ");
		}
		
		
		
		
		
		
		
	}

}
