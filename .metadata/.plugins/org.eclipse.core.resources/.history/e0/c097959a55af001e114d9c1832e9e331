package 이차원배열;

public class Ex02이차원배열예제1 {

	public static void main(String[] args) {

		int[][] array = new int[5][5];
		
		int num=21;
		for (int i = 0; i < array.length; i++) {
			array[i][0] = num++;
			
		}
		
		System.out.println(array[1][0]);

		array[0][1] = 21;
		array[1][1] = 22;
		array[2][1] = 23;
		array[3][1] = 24;
		array[4][1] = 25;
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
