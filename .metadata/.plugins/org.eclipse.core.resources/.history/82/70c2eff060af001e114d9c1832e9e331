package 이차원배열;

public class Ex03이차원배열예제2스네이크 {

	public static void main(String[] args) {

		int[][] array = new int[5][5];

		int num = 21;
		int i = 0;
		if (i % 2 == 0) {
			for (i = 0; i < array.length; i++) {
				array[0][i] = num++;
			}
		} else {
			for ( i = array.length; i > 0; i--) {
				array[1][i] = num++;
			}
		}

		
		System.out.println(array[1][2]);
		
		
		
	}

}
