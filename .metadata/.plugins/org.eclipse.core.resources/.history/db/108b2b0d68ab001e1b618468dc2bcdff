package 삼;

import java.util.Scanner;

public class Ex04 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.print("n 입력 : ");
		int num = sc.nextInt();
		int num1 =0;
		for(int i=1; i<num+1; i++) {
			int sum = 0;
			

			sum+= (i +num1);
			num1++ ;
			System.out.print(sum +" ");
		}
		
		
		
		
		
		
		
	}

}
