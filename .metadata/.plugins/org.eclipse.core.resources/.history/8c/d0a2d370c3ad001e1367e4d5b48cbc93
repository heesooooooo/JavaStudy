package 배열;

public class Ex01배열기초 {

	public static void main(String[] args) {

		// 배열(Array)의 자료구조
		// : 같은 타입의 여러 변수를 하나의 묶음으로 다루는 자료구조
		
		// 배열 선언하는 방법
		// 자료형[] 변수명 = new 자료형[0];
		int[] numbers = new int[5];
		System.out.println(numbers);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
