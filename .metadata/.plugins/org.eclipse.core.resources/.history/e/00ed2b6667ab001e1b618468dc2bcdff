package 이중for문;

public class Ex02별찍기 {

	public static void main(String[] args) {

		// *
		// **
		// ***
		// ****
		// *****
		
		for (int line = 1; line<6 ; line++) {
			for (int i=1; i<=line; i++) {System.out.print("*");
			
			}System.out.println();
		}
		

		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
