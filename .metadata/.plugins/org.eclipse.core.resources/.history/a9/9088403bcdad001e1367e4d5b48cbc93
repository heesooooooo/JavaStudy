package 배열;

import java.util.Random;

public class Ex03홀수찾기 {

	public static void main(String[] args) {

		Random ran = new Random();
		int ranNum = ran.nextInt(100)+1;
		
		int[] numbers = new int[5];
		for(int i =0; i<5; i++) {
		numbers[i] = ranNum;
		
		}
		for(int i=0; i<5; i++)
			{if(ranNum%2==1) {
			System.out.print(numbers[i] +" ");
		}
			}
		
		
		
		
	}

}
