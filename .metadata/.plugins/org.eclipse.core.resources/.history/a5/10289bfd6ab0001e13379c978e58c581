package day5;

import java.util.Random;

public class Ex01 {

	public static void main(String[] args) {

		int[] array = new int[8];
		Random ran = new Random();
		System.out.print("배열에 있는 모든 값 : ");
		for(int i =0; i<array.length; i++) {
			array[i] = ran.nextInt(100);
			System.out.print( array[i] + " ");
			
		}
		
		
		
	}

}
