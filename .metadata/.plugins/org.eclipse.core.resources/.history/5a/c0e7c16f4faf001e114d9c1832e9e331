package 이차원배열;

public class Ex01이차원배열생성 {

	public static void main(String[] args) {

		// 이차원 배열
		// : 배열 안에 또다른 배열이 있는 형태
		// : 참조가 2번 일어나는 형태 (참조의 참조다)
		
		// 1. 이차원 배열 선언방법
		// 자료형[][] 변수명 = new 자료형[행][열];
		int[][] array = new int[3][3];
		
		System.out.println(array[0][0]);
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
