package 이차원배열;

public class Ex03이차원배열예제2스네이크 {

	public static void main(String[] args) {

		int[][] array = new int[5][5];

		int num = 21;

		for (int i = 0; i < array.length; i++) {
			if (i % 2 == 0) {
				array[0][i] = num++;
			}
		}

		for (int i = array.length; i > 0; i--) {
			if (i % 2 == 1) {
				array[1][i] = num++;
			}
		}

		for (int j = 0; j < array.length; j++) {
			for (int i = 0; i < array.length; i++) {
				System.out.print(array[j][i] + "\t");
			}
			System.out.println();
		}
 
		
		
		
		
		
		
	}

}
