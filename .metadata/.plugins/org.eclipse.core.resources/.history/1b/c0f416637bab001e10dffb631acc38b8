package 이중for문;

public class Ex04공포의별찍기 {

	public static void main(String[] args) {

		for(int k=4; k>0; k--) 
		{for(int l=0; l<k; l++) {
			System.out.print(" ");
		}
		}
		
	
		
		
		for(int j =1; j<6; j++ ) {
			for(int k=4; k>j; k--) {
				System.out.print(" ");
			}
			
				for(int i=0; i<j ; i++) {
			System.out.print("*");
		}System.out.println();
		}
		
		
		
	}

}
