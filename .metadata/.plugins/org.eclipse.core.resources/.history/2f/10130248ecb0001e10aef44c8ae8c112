package OOP기초;

public class PersonMain {
	// 실행할 수 있는 공간
	public static void main(String[] args) {

		// 1. 설계도를 기반으로 객체를 생성
		// 자료형 변수명 = new 자료형();
		// 사람자료형을 만들기!! 변수명은 person
		Person person = new Person();
		System.out.println(person);
		System.out.println(person.name);
		
		
		
		
		
		
		
		
		
		
	}

}
