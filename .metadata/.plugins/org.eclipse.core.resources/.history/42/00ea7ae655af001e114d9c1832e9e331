package 이차원배열;

public class Ex02이차원배열예제1 {

	public static void main(String[] args) {

		int[][] array = new int[5][5];
		int num=21;
		for (int j = 0; j < array.length; j++) {
		for (int i = 0; i < array.length; i++) {
			array[i][j] = num++;
		}
		}

		
		
		
		System.out.println(array[1][1]);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
