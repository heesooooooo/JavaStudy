package 이중for문;

public class Ex02별찍기 {

	public static void main(String[] args) {

		// *
		// **
		// ***
		// ****
		// *****
		
		for (int line = 0; line<5 ; line++) {
			for (int i=0; i<line; i++) {System.out.print("*");
			
			}
		}
		System.out.println();

		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
