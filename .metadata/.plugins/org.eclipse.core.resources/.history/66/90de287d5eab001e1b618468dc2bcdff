package 삼;

public class Ex01 {

	public static void main(String[] args) {
		int i =1;
		for (i = 1; i < 50; i++) {
			if (i % 2 == 1) {
				System.out.print((i) + " ");
			} else {
				System.out.print("-" + i + " ");
			}
		}
		
		
		int sum=0;
		sum+=(2*i-1)-2*i;
		System.out.println();
		System.out.println("결과 : " +sum);

		
		
		
	}

}
